    "@angular/router": "^16.2.0",
    "bootstrap": "^5.2.3",
    "jquery": "^3.6.4",
    "jquery-validation": "^1.19.3",
    "jquery-validation-unobtrusive": "^3.2.12",
    "oidc-client": "^1.11.5",
